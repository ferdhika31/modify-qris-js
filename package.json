	"description": "Modify QRIS Generator is a javascript based libraries that enabling flexible payments with customizable amounts for improved efficiency and user experience",
	"main": "dist/index.js",
	"scripts": {
		"prepare": "husky install",
		"tsc": "tsc",
		"start:dev": "tsc -w",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ferdhika31/dynamic-qris-js.git"
	},
	"keywords": [
		"qris",
		"qr",
		"code",
		"indonesia",
		"payment",
		"dynamic",
		"dinamis"
	],
	"license": "MIT",
	"files": [
		"dist/**/*",
		"*.md"
	],
	"bugs": {
		"url": "https://github.com/ferdhika31/dynamic-qris-js/issues"
	},
	"homepage": "https://github.com/ferdhika31/dynamic-qris-js#readme",
	"devDependencies": {
		"@types/jest": "^29.5.5",
		"@types/node": "^20.6.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1"
	},
	"dependencies": {
		"canvas": "^3.1.0",
		"jsqr": "^1.4.0",
		"qrcode": "^1.5.4",
		"typescript": "^5.8.2"
	},
	"directories": {
		"doc": "docs",
		"example": "examples",
		"test": "tests"
